{
  "name": "angular-seed-hybrid-admin",
  "version": "0.0.3",
  "description": "Angular 2 Seed Hybrid Admin (Template)",
  "repository": {
    "url": "https://github.com/jvitor83/angular-seed-hybrid"
  },
  "scripts": {
    "build.dev": "gulp build.dev --color --config-env dev",
    "build.dev.watch": "gulp build.dev.watch --color",
    "build.e2e": "gulp build.e2e --color",
    "build.prod": "gulp build.prod --color --config-env prod",
    "build.prod.exp": "gulp build.prod.exp --color --config-env prod",
    "build.test": "gulp build.test --color",
    "test.watch": "gulp test.watch --color",
    "generate.manifest": "gulp generate.manifest --color",
    "clean": "rimraf -rf node_modules dist && cd src/cordova && rimraf -rf node_modules www temp && mkdir www",
    "e2e": "protractor",
    "e2e.live": "protractor --elementExplorer",
    "gulp": "gulp",
    "i18n": "ng-xi18n && gulp clean.i18n",
    "lint": "gulp tslint",
    "karma": "karma",
    "karma.start": "karma start",
    "postinstall": "gulp check.versions && gulp build.bundle.rxjs && npm prune && gulp webdriver && cd src/cordova && npm i && cd ../.. && node tools/install.js",
    "reinstall": "npm cache clean && npm install",
    "serve.coverage": "remap-istanbul -b src/ -i coverage/coverage-final.json -o coverage -t html && npm run gulp -- serve.coverage --color",
    "serve.dev": "gulp serve.dev --color --config-env dev",
    "serve.e2e": "gulp serve.e2e --color",
    "serve.prod": "gulp serve.prod --color --config-env prod",
    "start": "gulp serve.dev --color",
    "start.deving": "gulp start.deving --color",
    "build.dev.cordova": "gulp build.cordova --client cordova/app --dist src/cordova --dev-dest www --base=./",
    "build.prod.cordova": "npm run build.prod -- --client cordova/app --dist src/cordova --prod-dest www --base=./",
    "build.prod.exp.cordova": "npm run build.prod -- --client cordova/app --dist src/cordova --prod-dest www --base=./",
    "cordova.serve": "npm run build.dev.cordova && cd src/cordova && cordova serve",
    "start.windows": "npm run build.dev.cordova && cd src/cordova && cordova run windows",
    "install.windows": "cd src/cordova && cordova platform rm windows && cordova platform add windows",
    "start.ubuntu": "npm run build.dev.cordova && cd src/cordova && cordova run --device",
    "install.ubuntu": "cd src/cordova && cordova platform rm ubuntu && cordova platform add ubuntu --save",
    "start.ios": "npm run build.dev.cordova && cd src/cordova && cordova run ios",
    "install.ios": "cd src/cordova && cordova platform rm ios && cordova platform add ios",
    "start.osx": "npm run build.dev.cordova && cd src/cordova && cordova run",
    "install.osx": "cd src/cordova && cordova platform rm osx && cordova platform add osx",
    "start.android": "npm run build.dev.cordova && cd src/cordova && cordova run android",
    "install.android": "cd src/cordova && cordova platform rm android && cordova platform add android",
    "start.browser": "npm run build.dev.cordova && cd src/cordova && cordova run browser",
    "install.browser": "cd src/cordova && cordova platform rm browser && cordova platform add browser",
    "tasks.list": "gulp --tasks-simple --color",
    "test": "gulp test --color",
    "e2e.ci": "gulp build.prod --color && gulp build.js.e2e --color && gulp e2e --color",
    "tests.all": "npm test && npm run e2e.ci",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "author": "Jo√£o Vitor Paes de Barros do Carmo <jvitor83>",
  "contributors": [
    {
      "name": "Minko Gechev <mgechev>"
    },
    {
      "name": "Nathan Walker <NathanWalker>"
    }
  ],
  "license": "MIT",
